newspaper {
	title= "teste titulo oaoooopa"
	date= "24 de fevereiro de 2014"
	structure {
		col= 3
		show = headline1
	}
	
	headline1 {
		title= "teste news 1"
		date = "3 do 2 do 4"
		abstract = "blablablablabla blablabla
				:recuo 1
				::recuo 2
				*item1
				**item2
				#enum1
				#enum2"
		structure {
			col= 2
			show= abstract, date, text
		}
	}
}